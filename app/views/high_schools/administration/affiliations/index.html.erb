<%= content_for :jumbotron do %>
  <%= render partial: "high_schools/layout/jumbotron", locals: {presenter: @high_school_profile_presenter} %>
<% end %>

<%= content_for :navigation_links do %>
  <%= render partial: "high_schools/layout/navigation/administration_links", locals: {presenter: @high_school_profile_presenter} %>
<% end %>

<div class="container">
  <div>
    <p>
      <%= link_to "+ Invite New Coaches to Recruiting Board", [@high_school, :administration, :coach_invitations] %>
    </p>

    <p>
      <%= link_to "+ Invite Coaches Already on Recruiting Board", new_high_school_administration_affiliation_path(@high_school) %>
    </p>
  </div>

  <div class="coach-list">
    <h1>Your Lineup</h1>
    <%= render partial: "high_schools/administration/coaches/role_form", collection: @coaches, as: :coach  %>
  </div>
</div>
